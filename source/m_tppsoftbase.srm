type m_parametrosdeentidadporempresa from menu within m_sistema
end type

event clicked;boolean bValid
Int li_resp,li_ventanas,li_bucle,i
window wSheet[]
li_ventanas = 1
wSheet[li_ventanas] = ParentWindow.GetFirstSheet()

IF IsValid(wSheet[li_ventanas] ) THEN
	li_resp =  MessageBox(gn_config.is_work.aplicacion + " -- "+ Gn_Config.Is_Work.EmpresaDes,		"Hay ventanas abiertas, para abrir esta opcion las ventanas deben estar cerradas... "+Char(13) +"Desea que se cierren todas las ventanas para poder acceder a esta opcion del menu", Exclamation!, YesNoCancel!) 
	
	If li_resp = 1 Then

		DO
			li_ventanas++
			li_bucle++
			wSheet[li_ventanas]  = ParentWindow.GetNextSheet(wSheet[li_bucle])
			bValid = IsValid (wSheet[li_ventanas] )
//			IF bValid THEN Close(wSheet)
		LOOP WHILE bValid 
		//open
		For i=1 to li_ventanas 
			DO
				bValid = IsValid (wSheet[i] )
				IF bValid THEN Close(wSheet[i]) 
			LOOP WHILE bValid 
		Next
		
		IF NOT IsValid(w_manto_entidadparametros) THEN OpenSheet(w_manto_entidadparametros,w_frame,1,Original!)
	End If
Else 
	//open
     	IF NOT IsValid(w_manto_entidadparametros) THEN OpenSheet(w_manto_entidadparametros,w_frame,1,Original!)
END IF
end event

